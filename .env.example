# Sessionssäkerhetsnyckel - använd en slumpmässig sträng för att kryptera sessionsdata
SESSION_SECRET=

# Databasvärd - IP-adress eller domännamn för PostgreSQL-servern
DB_HOST=
# Databasport - standardport för PostgreSQL är 5432
DB_PORT=
# Databasnamn - namnet på din PostgreSQL-databas
DB_NAME=
# Databasanvändare - användarnamn för att ansluta till databasen
DB_USER=
# Databaslösenord - lösenord för databasanvändaren
DB_PASSWORD=

# Cloudflare R2 åtkomstnyckel - Access Key ID för R2 objektlagring
R2_ACCESS_KEY=
# Cloudflare R2 hemlig nyckel - Secret Access Key för R2 objektlagring
R2_SECRET_KEY=
# Cloudflare R2 konto-ID - ditt Cloudflare konto-ID
R2_ACCOUNT_ID=
# Cloudflare R2 bucket-namn - namnet på din R2 bucket för filuppladdningar
R2_BUCKET=